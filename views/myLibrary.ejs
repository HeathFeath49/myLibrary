		<h1>My Books</h1>
		<div id="tableContainer">
			<table id="bookTable" class="table">
				<thead>
					<tr>
						<th>Title</th>
						<th>Author</th>
					</tr>
				</thead>

				<tbody>

					<% for(i in books){  %>
						<tr>
							<td><%= JSON.stringify(books[i].Title)%></td>
							<td><%= JSON.stringify(books[i].Author)%></td>
						</tr>
				
					<% } %>
				</tbody>
			</table>
		</div>


